program ::= out | in ;
optmore ::= "{MORE}" ;
out ::= omsgs optmore? ;
in ::= imsgs ;
omsgs ::= omsg | omsg omsgs ;
imsgs ::= imsg | imsg imsgs ;
omsg ::= "To: " recipient "\n" !( "To: " )* ;
recipient ::= team ;
imsg ::= "From: " sender "\n" !( "From: " )* ;
sender ::= team | "User" ;
team ::= "Bob" | "Brandon" | "Bart" | "Eve" ;
